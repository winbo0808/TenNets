<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hh.tennets"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.hh.tennets.BaseApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
        <activity
            android:name="com.hh.tennets.activity.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hh.tennets.activity.MainActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.hh.tennets.activity.TextActivity"
            android:screenOrientation="portrait" >
        </activity>
        <!-- 注册 -->

        <activity
            android:name="com.hh.tennets.activity.RegisterActivity"
            android:screenOrientation="portrait" >
        </activity>
        <!-- 店铺收藏 -->

        <activity
            android:name="com.hh.tennets.activity.person.ShopCollectedActivity"
            android:screenOrientation="portrait" >
        </activity>

        <!-- 我的订单 -->

        <activity
            android:name="com.hh.tennets.activity.person.myorder.MyOrdersActivity"
            android:screenOrientation="portrait" >
        </activity>
        <!-- 订单详情 -->

        <activity
            android:name="com.hh.tennets.activity.person.myorder.MyOrderDetailsActivity"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>