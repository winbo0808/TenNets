        <activity
            android:name="com.hh.tennets.activity.person.myshop.MyShopSettingActivity"
            android:screenOrientation="portrait" >
        </activity>
        <!-- 咨询详情 -->
        <activity
            android:name="com.hh.tennets.activity.person.myshop.MyShopNewsDetailsActivity"
            android:screenOrientation="portrait" >
        </activity>

        <!-- 开店申请 -->
        <activity
            android:name="com.hh.tennets.activity.person.myshop.ApplyOpenShopActivity"
            android:screenOrientation="portrait" >
        </activity>

        <!-- 开店详情 -->
        <activity
            android:name="com.hh.tennets.activity.person.myshop.OpenShopDetailsActivity"
            android:screenOrientation="portrait" >
        </activity>

        <!-- 开店协议 -->
        <activity
            android:name="com.hh.tennets.activity.person.myshop.OpenShopAgreementActivity"
            android:screenOrientation="portrait" >
        </activity>
        <!-- 审核状态 -->
        <activity
            android:name="com.hh.tennets.activity.person.myshop.OpenShopReviewStatusActivity"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>